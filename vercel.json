{
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "portfolio-bimawp.vercel.app"
        }
      ],
      "destination": "https://bimawiryadipraja.my.id/:path*",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.bimawiryadipraja.my.id"
        }
      ],
      "destination": "https://bimawiryadipraja.my.id/:path*",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
