{
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "portfolio-bimawp.vercel.app"
        }
      ],
      "destination": "https://bimawiryadipraja.my.id/$1",
      "permanent": true
    }
  ]
}
